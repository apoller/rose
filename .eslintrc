{
    "extends": "standard",
    "parser": "babel-eslint",
    "rules": {
        "arrow-parens": 0,
        "no-console": 0,
        "indent": [2, 4, {"SwitchCase": 1}],
        "babel/arrow-parens": 2
    },
    "globals": {
        "Backbone": true,
        "kango": true
    },
    "plugins": [
        "babel"
    ]
}
