        ]
    },
    {
        "name": "share-status",
        "network": "facebook",
        "type": "click",
        "priority": 14,
        "version": "0.2",
        "patterns": [
            {
                "node": "a[ajaxify*=\"share/dialog/submit\"] *, a[ajaxify*=\"share/dialog/submit\"]",
                "container": "a"
            },
            {
                "node": "form[action*=\"sharer/submit\"] button[type=\"submit\"]",
                "container": "form[action*=\"sharer/submit\"]"
            }
        ]
    },
    {
        "name": "create-event",
        "network": "facebook",
        "type": "click",
