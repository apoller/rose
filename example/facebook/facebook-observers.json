        "patterns": [
            {
                "node": "a[ajaxify*=\"/associate.php\"] *, a[ajaxify*=\"/associate.php\"]",
                "container": "a"
            }
        ]
    },
    {
        "name": "decline-event",
        "network": "facebook",
        "type": "click",
        "priority": 18,
        "version": "0.2",
        "patterns": [
            {
                "node": "a[ajaxify*=\"/decline.php\"] *, a[ajaxify*=\"/decline.php\"]",
