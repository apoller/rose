    {
        "name": "like-photo",
        "network": "facebook",
        "type": "click",
        "priority": 8,
        "version": "0.3",
        "patterns": [
            {
                "node": ".fbPhotosPhotoLike span.like",
                "container": ".fbPhotoSnowliftPopup"
            },
            {
                "node": ".UFILikeLink span:contains(\"Like\"), .UFILikeLink span:contains(\"Gefällt mir\")",
